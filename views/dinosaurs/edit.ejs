<form method="POST" action="/dinosaurs/<%=dinoId%>/?_method=PUT">
    <label for="name">Name</label>
    <input type="text" name="name" name="<%= dino.name %>">
    <label for="type">Type</label>
    <input type="text" name="type" name="<%= dino.type %>">
    <input type="submit" value="Update Dino!">
</form>